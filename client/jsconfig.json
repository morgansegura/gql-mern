{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "node_modules/*": ["../node_modules/*"],
            "context/*": ["context/*"],
            "pages/*": ["pages/*"],
            "components/*": ["components/*"],
            "core/*": ["components/core/*"],
            "styled/*": ["styled/*"]
        }
    },
    "include": ["src"],
    "exclude": ["node_modules"],
    "typeAcquisition": {
        "exclude": [ "dotenv" ]
    }
}
